{
  "name": "@kyaku/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "dotenv -e ../../.env tsx src/seed.ts",
    "clean": "git clean -xdf .cache .turbo node_modules",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@kyaku/kyaku": "*",
    "drizzle-orm": "^0.36.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@kyaku/eslint-config": "*",
    "@kyaku/prettier-config": "*",
    "@kyaku/tsconfig": "*",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.12",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.27.1",
    "eslint": "^9.13.0",
    "lexical": "^0.19.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "prettier": "@kyaku/prettier-config"
}
