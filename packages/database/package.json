  "devDependencies": {
    "@cs/eslint-config": "*",
    "@cs/prettier-config": "*",
    "@cs/tsconfig": "*",
    "@types/ws": "^8.5.10",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.11",
    "eslint": "^8.56.0",
    "lexical": "^0.12.6",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cs/eslint-config/base"
