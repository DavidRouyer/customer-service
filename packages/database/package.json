  "devDependencies": {
    "@kyaku/eslint-config": "*",
    "@kyaku/prettier-config": "*",
    "@kyaku/tsconfig": "*",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.12",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.11.1",
    "lexical": "^0.17.1",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "prettier": "@kyaku/prettier-config"
