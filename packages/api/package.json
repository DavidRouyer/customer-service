{
  "name": "@cs/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./graphql": "./src/graphql.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo node_modules",
    "codegen:graphql": "graphql-codegen",
    "codegen:graphql:watch": "graphql-codegen --watch",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cs/auth": "*",
    "@cs/database": "*",
    "@cs/kyaku": "*",
    "@graphql-tools/merge": "^9.0.4",
    "@graphql-tools/schema": "^10.0.3",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "awilix": "^10.0.2",
    "dataloader": "^2.2.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.23.0",
    "superjson": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cs/eslint-config": "*",
    "@cs/prettier-config": "*",
    "@cs/tsconfig": "*",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@parcel/watcher": "^2.4.1",
    "eslint": "^8.57.0",
    "lexical": "^0.15.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@cs/eslint-config/base"
    ]
  },
  "prettier": "@cs/prettier-config"
}
